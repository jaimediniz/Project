{
  "rest-client.environmentVariables": {
    "$shared": {
      "version": "v0",
      "prodToken": "foo",
      "nonProdToken": "bar"
    },
    "local": {
      "version": "v0",
      "host": "localhost:3000",
      "token": "{{$shared nonProdToken}}",
      "secretKey": "devSecret"
    },
    "production": {
      "host": "localhost:3000",
      "token": "{{$shared prodToken}}",
      "secretKey": "prodSecret"
    }
  }
}
